<div class="show-data">
  <div class="header">
      <LinkTo @route="products" ><i class="fa fa-angle-left"></i></LinkTo>
      <div class="header-content">
          <img src="../assets/avatar.png" alt="avatar">
          <span class="title">Data Publisher</span>
      </div>
      
  </div>
  <div class="data-title">
    <span>{{product.name}}</span>
    <div class="data-button">
      <button class="coverage" {{action "select" "coverage"}}>coverage</button>
      <button class="download" {{action "select" "download"}}>download<span>&#9660;</span></button>
    </div>
  </div>
  <div class="data-body">
    <div class="body-title">
      <a class="nav-link active" href="#"{{action "select" "data sample"}}>data sample</a>
      <a class="nav-link" href="#"{{action "select" "coverage"}}>coverage</a>
    </div>
    <div class="data-table">
      {{ product-data 
        product = productData 
        page = "show" }}
    </div>
  </div>
</div>
{{outlet}}

